        <mxCell id="deTtVbom349j7HBjlkId-1761" parent="1" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f5f5f5;fontColor=#333333;strokeColor=#666666;" value="&lt;b&gt;Configuration Service&lt;/b&gt;&lt;div&gt;&lt;i&gt;Allows for configuration of certain things within services without having to restart the application.&lt;/i&gt;&lt;/div&gt;" vertex="1">
